{
  "name": "openclaw-configgen",
  "version": "0.0.0",
  "private": true,
  "description": "Generate and lint OpenClaw configs (WIP)",
  "type": "module",
  "bin": {
    "openclaw-configgen": "./bin/openclaw-configgen.js"
  },
  "scripts": {
    "gen:docs": "node scripts/gen-lint-rules-md.js",
    "lint:config": "node scripts/gen-lint-rules-md.js && node bin/openclaw-configgen.js lint",
    "lint:config:json": "node scripts/gen-lint-rules-md.js && node bin/openclaw-configgen.js lint",
    "selftest": "npm run gen:docs && node scripts/selftest.js"
  },
  "license": "MIT"
}
